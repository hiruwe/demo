pipeline {
    agent { docker { image 'maven:3.3.3' } }
    stages {
        stage('Pull') {
            steps {
                sh 'mvn --version'
            }
        } stage('build') {
        	steps {
                sh 'mvn --version'
            }
        }stage('Deploy') {
        	steps {
                sh 'mvn --version'
            }
        }
    }
}